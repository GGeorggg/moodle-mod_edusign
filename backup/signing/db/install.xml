<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/edusign/submission/signing/db" VERSION="20120423" COMMENT="XMLDB file for Moodle mod/edusign/submission/signing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="edusignsubmission_signing" COMMENT="Info about signing submission">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="edusignment" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="submission" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="signing" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="The text for this online text submission."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for this signing submission."/>
        <KEY NAME="edusignment" TYPE="foreign" FIELDS="edusignment" REFTABLE="edusign" REFFIELDS="id" COMMENT="The edusignment instance this online text submission relates to."/>
        <KEY NAME="submission" TYPE="foreign" FIELDS="submission" REFTABLE="edusign_submission" REFFIELDS="id" COMMENT="The submission this online text submission relates to."/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
